{
    "name": "rustube-music",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "prepare": "svelte-kit sync || echo ''",
        "postinstall": "pnpm run shaka:patch",
        "tauri": "tauri",
        "auth:init": "scripts/auth/init.ts",
        "innertube:proxy": "scripts/innertube/proxy.ts",
        "shaka:patch": "scripts/shaka/patch.ts",
        "dev": "pnpm run auth:init && pnpx concurrently 'pnpm run innertube:proxy' 'vite dev'",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write ${FILES:-.} && eslint --fix ${FILES:-.}",
        "lint": "prettier --check . && eslint .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run"
    },
    "dependencies": {
        "@tauri-apps/api": "^2.9.0",
        "@tauri-apps/plugin-opener": "~2.5.0",
