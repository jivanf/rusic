        "googlevideo": "^4.0.4",
        "parse-full-name": "^1.2.6",
        "rxjs": "^7.8.2",
        "shaka-player": "^4.16.6",
        "sveltefire": "^0.4.5",
        "youtubei.js": "^16.0.1"
